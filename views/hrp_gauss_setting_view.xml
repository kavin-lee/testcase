<odoo>
    <data>
        <record id="hrp_gauss_demand_people_setting_tree" model="ir.ui.view">
            <field name="name">hrp.gauss.demand.people.setting.tree</field>
            <field name="model">hrp.gauss.demand.people.setting</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="人员设定tree" editable="bottom">
                    <field name='name'/>
                    <field name='type'/>
                    <field name='is_owner'/>
                </tree>
            </field>
        </record>

        <record id="action_hrp_gauss_demand_people_setting" model="ir.actions.act_window">
            <field name="name">人员设置</field>
            <field name="res_model">hrp.gauss.demand.people.setting</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="hrp_gauss_demand_people_setting_tree"/>
        </record>

        <menuitem parent="hrp_gauss_settings_menu_root"
                  action="action_hrp_gauss_demand_people_setting"
                  id="hrp_gauss_demand_people_setting_menu"/>
    </data>
</odoo>
